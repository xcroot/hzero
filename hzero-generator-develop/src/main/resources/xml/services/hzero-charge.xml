                <sql>SELECT
                    template_id,
                    template_code,
                    template_name,
                    template_title,
                    template_content,
                    message_category_code,
                    message_subcategory_code,
                    external_code,
                    sql_value,
                    lang,
                    enabled_flag,
                    tenant_id
