                    executor_id,
                    job_code,
                    job_cron,
                    description,
                    job_param,
                    alarm_email,
                    executor_strategy,
                    fail_strategy,
                    glue_type,
                    job_handler,
                    cycle_flag,
                    start_date,
                    end_date,
                    tenant_id,
                    parent_id,
                    strategy_param,
                    serial,
                    expand_param,
                    init_flag
                    FROM
                    hsdr_job_info
